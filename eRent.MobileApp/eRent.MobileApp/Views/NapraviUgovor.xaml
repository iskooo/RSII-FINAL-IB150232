<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="travelAworld.MobileApp.Views.NapraviUgovor">
    <ContentPage.Content>
        <StackLayout>
            <Button IsEnabled="{Binding btnEnabled}" x:Name="prihvati" Margin="0,10,0,0" Text="PRIHVATI I ZAKLJUČI UGOVOR"
                        Pressed="prihvati_Pressed"
                        BackgroundColor="DarkGreen"
                        TextColor="White" />
            <Button IsEnabled="{Binding btnEnabled}" x:Name="odbij" Margin="0,10,0,0" Text="ODBIJ"
                        Pressed="odbij_Pressed"
                        BackgroundColor="IndianRed"
                        TextColor="White" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>